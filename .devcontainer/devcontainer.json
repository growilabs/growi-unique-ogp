{
  "name": "GROWI-OGP-Dev",
  "dockerComposeFile": "docker-compose.yml",
  "service": "node",
  "workspaceFolder": "/workspace/growi-ogp",
  "settings": {
    "eslint.workingDirectories": ["/workspace/growi-ogp"],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "mhutchie.git-graph",
    "eamodio.gitlens",
    "github.vscode-pull-request-github",
    "cschleiden.vscode-github-actions"
  ],
  "remoteUser": "node"
}
